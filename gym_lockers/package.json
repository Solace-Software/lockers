{
  "name": "gym-locker-admin-dashboard",
  "version": "2.0.0",
  "description": "A management system for gym lockers with MQTT support",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server-direct\" \"npm run client\"",
    "server": "node start-server.js",
    "server-dev": "nodemon server.js",
    "server-direct": "node server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node start-server.js",
    "clean-start": "node start-server.js",
    "cleanup": "node cleanup-ports.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mqtt": "^5.3.5",
    "mysql2": "^3.9.7",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "mqtt",
    "gym",
    "locker",
    "admin",
    "dashboard"
  ],
  "author": "Solace Software",
  "license": "MIT"
}