{
  "name": "Gym Locker Admin Dashboard",
  "version": "1.1.1",
  "slug": "gym_locker_dashboard",
  "description": "Manage gym lockers with MQTT integration. Uses MariaDB for storage. Fixed MQTT message flood issues for improved stability.",
  "repository": "https://github.com/Solace-Software/lockers",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:3001]/",
  "ports": {
    "3001/tcp": 3001
  },
  "ingress": true,
  "ingress_entry": "/",
  "panel_icon": "mdi:lock",
  "arch": ["amd64", "armv7", "aarch64"],
  "options": {
    "db_host": "core-mariadb",
    "db_port": 3306,
    "db_user": "your_user",
    "db_password": "your_password",
    "db_name": "gym_lockers",
    "mqtt_host": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_username": "",
    "mqtt_password": "",
    "mqtt_client_id": "gym-admin"
  },
  "schema": {
    "db_host": "str",
    "db_port": "int",
    "db_user": "str",
    "db_password": "str",
    "db_name": "str",
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_username": "str",
    "mqtt_password": "str",
    "mqtt_client_id": "str"
  }
} 