{
  "name": "Gym Locker Admin Dashboard",
  "version": "1.4.2",
  "slug": "gym_locker_dashboard",
  "description": "Complete gym locker management system with internal database and optional MQTT configuration. No external dependencies required - everything runs internally.",
  "repository": "https://github.com/Solace-Software/lockers",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:3001]/",
  "ports": {
    "3001/tcp": 3001
  },
  "ingress": true,
  "ingress_entry": "/",
  "panel_icon": "mdi:lock",
  "arch": ["amd64", "armv7", "aarch64"],
  "options": {
    "mqtt_enabled": false,
    "use_external_mqtt": false,
    "external_mqtt_host": "",
    "external_mqtt_port": 1883,
    "external_mqtt_username": "",
    "external_mqtt_password": "",
    "external_mqtt_client_id": "gym-admin-external",
    "system_auto_refresh": 30,
    "system_data_retention_days": 90,
    "system_backup_enabled": true,
    "system_debug_mode": false
  },
  "schema": {
    "mqtt_enabled": "bool",
    "use_external_mqtt": "bool",
    "external_mqtt_host": "str",
    "external_mqtt_port": "int(1,65535)",
    "external_mqtt_username": "str",
    "external_mqtt_password": "str",
    "external_mqtt_client_id": "str",
    "system_auto_refresh": "int(5,300)",
    "system_data_retention_days": "int(1,3650)",
    "system_backup_enabled": "bool",
    "system_debug_mode": "bool"
  },
  "map": [
    "data:rw"
  ],
  "environment": {
    "DB_HOST": "localhost",
    "DB_PORT": "3306",
    "DB_USER": "gym_admin",
    "DB_NAME": "gym_lockers"
  }
} 