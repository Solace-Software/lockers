{
  "name": "Gym Locker Admin Dashboard",
  "version": "1.0.7",
  "slug": "gym_locker_dashboard",
  "description": "Manage gym lockers with MQTT integration. Uses MariaDB for storage.",
  "repository": "https://github.com/Solace-Software/lockers",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:3001]",
  "ports": {
    "3001/tcp": 3001
  },
  "ingress": false,
  "panel_icon": "mdi:lock",
  "arch": ["amd64", "armv7", "aarch64"],
  "options": {
    "db_host": "core-mariadb",
    "db_port": 3306,
    "db_user": "your_user",
    "db_password": "your_password",
    "db_name": "gym_lockers"
  },
  "schema": {
    "db_host": "str",
    "db_port": "int",
    "db_user": "str",
    "db_password": "str",
    "db_name": "str"
  }
} 