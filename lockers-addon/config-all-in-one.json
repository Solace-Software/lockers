{
  "name": "Gym Locker Admin Dashboard - All-in-One",
  "version": "1.4.7",
  "slug": "gym_locker_dashboard_all_in_one",
  "description": "Complete gym locker management system with integrated MQTT broker and database. Everything in one addon.",
  "repository": "https://github.com/Solace-Software/lockers",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:3001]/",
  "ports": {
    "3001/tcp": 3001,
    "1883/tcp": 1883,
    "9001/tcp": 9001,
    "3306/tcp": 3306
  },
  "ingress": true,
  "ingress_entry": "/",
  "panel_icon": "mdi:lock",
  "arch": ["amd64", "armv7", "aarch64"],
  "options": {
    "db_password": "secure_password_123",
    "mqtt_username": "gym_mqtt_user",
    "mqtt_password": "mqtt_password_123",
    "mqtt_client_id": "gym-admin-all-in-one",
    "mqtt_websocket_port": 9001,
    "mqtt_allow_anonymous": true,
    "mqtt_max_connections": 100,
    "mqtt_max_message_size": 1024,
    "system_auto_refresh": 30,
    "system_data_retention_days": 90,
    "system_backup_enabled": true,
    "system_debug_mode": false,
    "security_session_timeout": 30,
    "security_password_policy": "standard",
    "security_two_factor_auth": false,
    "security_audit_logging": true
  },
  "schema": {
    "db_password": "str",
    "mqtt_username": "str",
    "mqtt_password": "str",
    "mqtt_client_id": "str",
    "mqtt_websocket_port": "int(1,65535)",
    "mqtt_allow_anonymous": "bool",
    "mqtt_max_connections": "int(1,1000)",
    "mqtt_max_message_size": "int(1,65535)",
    "system_auto_refresh": "int(5,300)",
    "system_data_retention_days": "int(1,3650)",
    "system_backup_enabled": "bool",
    "system_debug_mode": "bool",
    "security_session_timeout": "int(5,480)",
    "security_password_policy": "list(standard|strong|enterprise)",
    "security_two_factor_auth": "bool",
    "security_audit_logging": "bool"
  }
} 